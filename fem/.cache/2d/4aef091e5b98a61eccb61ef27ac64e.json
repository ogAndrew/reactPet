data.petfinder.pet.description,\n          media: data.petfinder.pet.media,\n          breed,\n          loading: false\n        });\n      })\n      .catch(() => {\n        navigate(\"/\");\n      });\n  }\n  toggleModal = () => this.setState({ showModal: !this.state.showModal });\n\n  render() {\n    if (this.state.loading) {\n      return <h1>loading … </h1>;\n    }\n\n    const { media, animal, breed, location, description, name, showModal } = this.state;\n\n    return (\n      <div className=\"details\">\n        <Carousel media={media} />\n        <div>\n          <h1>{name}</h1>\n          <h2>{`${animal} — ${breed} — ${location}`}</h2>\n          <button onClick={this.toggleModal}>Adopt {name}</button>\n          <p>{description}</p>\n          {\n            showModal ? (\n              <Modal>\n                <h1>Would you like to adopt {name}?</h1>\n                <div className=\"buttons\">\n                  <button onClick={this.toggleModal}>Yes</button>\n                  <button onClick={this.toggleModal}>Ok Yes</button>\n                </div>\n              </Modal>\n            ) : null }\n    \n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Details;\n"},"lineCount":null}},"hash":"d7cdf3059ed8b035daf2bdfac68ed92c","cacheData":{"env":{"API_KEY":"8dac02807bd4e7776af5b9e50bbba3be","API_SECRET":"bdc881350a469342e6ab50fb1ac883e5"}}}